  "scripts": {
    "build": "yarn run compile && remix build",
    "compile": "node ./.compile.js",
    "dev": "concurrently \"yarn run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/base.css -o ./app/tailwind.css -w",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "postinstall": "remix setup node",
    "prebuild": "tailwindcss -i ./app/styles/base.css -o ./app/tailwind.css",
    "predev": "yarn run compile && yarn run prebuild",
    "start": "remix-serve build",
    "type-check": "tsc --project ./tsconfig.json"
